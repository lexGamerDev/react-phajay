  },
  "homepage": "https://github.com/phajay/phajay-payment-sdk#readme",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "rollup": "^3.29.2",
    "rollup-plugin-typescript2": "^0.35.0",
